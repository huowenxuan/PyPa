            <div class="post_info">
                <div class="a_con_text cont" id="pnl_contentinfo" ownerid="17148049" owner="satv" dataid="7702724">
                    <div class="summary">
                                                        
                            
                        
                        <div id="exinfo-kwinfo" data-cs-t="ginfo_kw_hotel" data-hotelsnum="0"></div>
